@article{haririExtendedIsolationForest2021,
  title = {Extended {{Isolation Forest}}},
  author = {Hariri, Sahand and Kind, Matias Carrasco and Brunner, Robert J.},
  year = {2021},
  month = apr,
  journal = {IEEE Transactions on Knowledge and Data Engineering},
  volume = {33},
  number = {4},
