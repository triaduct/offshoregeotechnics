  title = {Reliability Updating for Lateral Failure of Historic Quay Walls},
  author = {Hemel, Mart-Jan and , Dirk Jan, Peters and , Timo, Schweckendiek and {and Jonkman}, Sebastiaan N.},
  year = {2024},
