@article{lheureuxPredictionSettlementHighresolution2013,
  title = {On the Prediction of Settlement from High-Resolution Shear-Wave Reflection Seismic Data: {{The Trondheim}} Harbour Case Study, Mid {{Norway}}},
  shorttitle = {On the Prediction of Settlement from High-Resolution Shear-Wave Reflection Seismic Data},
  author = {L'Heureux, J. S. and Long, M. and Vanneste, M. and Sauvin, G. and Hansen, L. and Polom, U. and Lecomte, I. and Dehls, J. and Janbu, N.},
