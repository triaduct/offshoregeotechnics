  pages = {957--973},
  publisher = {ICE Publishing},
  issn = {0016-8505},
  doi = {10.1680/jgeot.20.P.117},
  urldate = {2025-01-08},
  abstract = {This paper examines the effect of consolidation on a shallowly embedded horizontal plate anchor in medium dense and dense sand using centrifuge tests, where the loading rate was varied over four orders of magnitude. The experimental results show a 4{$\cdot$}8 to 5{$\cdot$}5 times increase in anchor capacity as the consolidation condition changes from drained to undrained, driven by a steadily increasing negative excess pore pressure with increasing loading rate. At the highest loading rate, under undrained conditions, the measured maximum negative excess pore pressures reach a steady limit, suggesting the occurrence of cavitation. This increase in anchor capacity with dimensionless velocity is captured using a `backbone curve' interpretation framework that describes the change in capacity between the limiting values of drained and undrained anchor capacity. Calculation of drained anchor capacity is straightforward relative to the more challenging problem of calculating undrained capacity, particularly during cavitation. This was addressed separately through a numerical parametric study (pure undrained analysis) using a bounding surface soil model involving different water depths (cavitation potential), densities and embedment ratios. The numerical results are then synthesised into a simple extended analytical solution to allow estimation of undrained anchor capacity under different densities and water depths.},
  keywords = {bounding surface model,cavitation,coastal marine and offshore geotechnics,consolidation,foundations and soil-structure interaction,numerical modelling,physical modelling,plate anchors,sand},
  file = {C:\Users\kduffy\Zotero\storage\5XGGHQ7Z\Roy et al. - 2022 - Consolidation effects on uplift capacity of shallow horizontal plate anchors in dilating sand.pdf}
}

@article{ruggeriRenovationQuayWalls2019,
  title = {Renovation of Quay Walls to Meet More Demanding Requirements: {{Italian}} Experiences},
  shorttitle = {Renovation of Quay Walls to Meet More Demanding Requirements},
