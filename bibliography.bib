@article{newmanIndustry40Deployment2020,
  title = {Industry 4.0 Deployment in the Construction Industry: A Bibliometric Literature Review and {{UK-based}} Case Study},
  shorttitle = {Industry 4.0 Deployment in the Construction Industry},
  author = {Newman, Chris and Edwards, David and Martek, Igor and Lai, Joseph and Thwala, Wellington Didibhuku and Rillie, Iain},
