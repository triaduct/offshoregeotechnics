@article{dohertyFieldValidationFibre2015,
  title = {Field Validation of Fibre {{Bragg}} Grating Sensors for Measuring Strain on Driven Steel Piles},
  author = {Doherty, P. and Igoe, David John Paul and Murphy, G. and Gavin, Kenneth G. and Preston, J. and McAVOY, C. and Byrne, B. W. and Mcadam, R. and Burd, H. J. and Houlsby, G. T. and Martin, C. M. and Zdravkovi{\'c}, L. and Taborda, D. M. G. and Potts, D. M. and Jardine, R. J. and Sideri, M. and Schroeder, F. C. and Muir Wood, A. and Kallehave, D. and Skov Gretlund, J.},
  year = {2015},
