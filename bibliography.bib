@article{degansLateSaalianEemian2000,
  title = {Late {{Saalian}} and {{Eemian}} Deposits in the {{Amsterdam}} Glacial Basin},
  author = {{de Gans}, Wim and Beets, Dirk J. and Centineo, Maria Carla},
  year = {2000},
