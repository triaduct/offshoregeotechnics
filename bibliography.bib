  title = {The Formation of Authigenic Deposits during {{Paleogene}} Warm Climatic Intervals: A Review},
  shorttitle = {The Formation of Authigenic Deposits during {{Paleogene}} Warm Climatic Intervals},
  author = {Banerjee, Santanu and Choudhury, Tathagata Roy and Saraswati, Pratul Kumar and Khanolkar, Sonal},
