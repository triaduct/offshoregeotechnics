@article{dysonEvaluatingClayStiffness2025,
  title = {Evaluating Clay Stiffness Effects on Offshore Pile Running with the {{Coupled Eulerian Lagrangian Method}}},
  author = {Dyson, A. P. and Tolooiyan, A. and Gavin, Kenneth G.},
  year = {2025},
