  shorttitle = {Revised {{Upper Cenozoic}} Stratigraphy of the {{Dutch}} Sector of the {{North Sea Basin}}},
  author = {Rijsdijk, K. F. and Passchier, S. and Weerts, H. J. T. and Laban, C. and van Leeuwen, R. J. W. and Ebbing, J. H. J.},
  year = {2005},
