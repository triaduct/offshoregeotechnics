@article{robertsonEvaluationFlowLiquefaction2021,
  title = {Evaluation of Flow Liquefaction and Liquefied Strength Using the Cone Penetration Test: An Update},
  shorttitle = {Evaluation of Flow Liquefaction and Liquefied Strength Using the Cone Penetration Test},
  author = {Robertson, Peter K. J.},
