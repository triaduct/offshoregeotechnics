@article{luoOptimisingSuctionBucket2025,
  title = {Optimising Suction Bucket Foundation Installation for Offshore Renewable Energy Infrastructure through Field Data on Self-Weight Penetration},
  author = {Luo, Lunbo and Zhou, Xingzheng and Zhou, Zefeng and Wang, Weichen and Zhang, Xianwei and Li, Jinhui},
  year = {2025},
