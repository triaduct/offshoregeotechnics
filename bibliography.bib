@article{piccoloMechanicalPropertiesOptical2020,
  title = {Mechanical {{Properties}} of {{Optical Fiber Strain Sensing Cables}} under {$\gamma$}-{{Ray Irradiation}} and {{Large Strain Influence}}},
  author = {Piccolo, Arianna and {Delepine-Lesoille}, Sylvie and Friedrich, Etienne and Aziri, Shasime and Lecieux, Yann and Leduc, Dominique},
