@article{ge25000TEUVessels2021,
  title = {Towards 25,000 {{TEU}} Vessels? {{A}} Comparative Economic Analysis of Ultra-Large Containership Sizes under Different Market and Operational Conditions},
  shorttitle = {Towards 25,000 {{TEU}} Vessels?},
  author = {Ge, Jiawei and Zhu, Mo and Sha, Mei and Notteboom, Theo and Shi, Wenming and Wang, Xuefeng},
  year = {2021},
