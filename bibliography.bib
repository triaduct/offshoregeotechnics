@article{cathieAxialCapacityAgeing2023,
  title = {Axial Capacity Ageing Trends of Large Diameter Tubular Piles Driven in Sand},
  author = {Cathie, D. and Jardine, R. and Silvano, R. and Kontoe, S. and Schroeder, F.},
  year = {2023},
